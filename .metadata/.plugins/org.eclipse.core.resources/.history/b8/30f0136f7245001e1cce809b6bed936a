package in.mindcraft.controllers;

import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;


@Controller

public class LaptopController {
	@RequestMapping("/insertlap")
	public void addLaptop(HttpServlet request, HttpServletResponse response)
	{
		int lid = Interger.parseInt(request.getParameter("lid"));
		String make= request.getParameter("make";)
				double cost = Double.parseDouble(request.getParameter("cost"));
		
		Laptop laptop = new Laptop(lid,make,cost);
	}

}
